[Unit]
Description=Courselore
After=network.target

[Service]
ExecStart=/root/courselore/app/node_modules/.bin/node /root/courselore/app/lib/index.js /root/courselore/app/deployment-example/configuration.js
Environment=NODE_ENV=production
User=root
Restart=always

[Install]
WantedBy=multi-user.target
