{
  "name": "courselore",
  "version": "0.0.1",
  "scripts": {
    "develop": "ts-node-dev --files src/index.ts",
    "test": "prettier --check 'src/**/*' 'static/**/*'",
    "build": "tsc && pkg --targets latest --out-path pkg .",
    "clean": "shx rm -rf lib pkg"
  },
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "dependencies": {
    "express": "^4.17.1",
    "greenlock": "^4.0.4",
    "greenlock-express": "^4.0.3",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.21",
    "@types/shelljs": "^0.8.8",
    "@types/sqlite3": "^3.1.6",
    "jest": "^26.6.3",
    "pkg": "github:leafac/pkg",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "pkg": {
    "assets": [
      "static/**/*"
    ]
  }
}
