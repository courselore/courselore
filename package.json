{
  "name": "courselore",
  "version": "0.0.1",
  "scripts": {
    "develop": "nodemon --ext 'ts' --exec 'ts-node src/index.ts'",
    "test": "prettier --check 'src/**/*.{ts}'",
    "build": "tsc && pkg --targets latest-macos,latest-linux,latest-win --out-path pkg .",
    "clean": "rimraf lib pkg"
  },
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "dependencies": {
    "express": "^4.17.1",
    "sqlite": "^4.0.19",
    "sqlite3": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.21",
    "@types/sqlite3": "^3.1.6",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pkg": "github:leafac/pkg",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "pkg": {
    "assets": [
      "static/**/*"
    ]
  }
}
